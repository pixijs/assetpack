    "@oclif/test": "^2.2.10",
    "@types/node": "^16.18.3",
    "oclif": "^3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "oclif": {
    "bin": "assetpack",
    "dirname": "assetpack",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " "
  }
