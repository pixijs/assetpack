{
  "name": "@assetpack/cli",
  "version": "0.0.0",
  "description": "A simple CLI to run AssetPack",
  "keywords": [
    "oclif"
  ],
  "homepage": "https://github.com/pixijs/assetpack/tree/master/packages/cli/#readme",
  "bugs": "https://github.com/pixijs/assetpack/issues",
  "repository": {
    "url": "pixijs/assetpack",
    "directory": "packages/cli"
  },
  "license": "MIT",
  "author": "Zyie",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "assetpack": "./bin/run"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "rimraf dist && tsc -b",
    "lint": "",
    "prepack": "npm run build && oclif manifest",
    "postpack": "rimraf oclif.manifest.json"
  },
  "dependencies": {
    "@oclif/core": "^2.0.8",
    "@oclif/plugin-help": "^5",
    "chalk": "^4.1.2",
    "fs-extra": "^11.1.0",
    "@assetpack/core": ">=0"
  },
  "devDependencies": {
    "@oclif/test": "^2.2.20",
    "@types/node": "^16.18.11",
    "oclif": "^3",
    "ts-node": "^10.9.1"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "oclif": {
    "bin": "assetpack",
    "dirname": "assetpack",
    "default": ".",
    "commands": "./dist",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  }
}
