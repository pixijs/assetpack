{
  "name": "@assetpack/cli",
  "version": "0.0.0",
  "description": "",
  "homepage": "https://github.com/pixijs/assetpack/tree/master/packages/assetpack-cli/#readme",
  "bugs": "https://github.com/pixijs/assetpack/issues",
  "repository": {
    "url": "pixijs/assetpack",
    "directory": "packages/assetpack-cli"
  },
  "license": "MIT",
  "author": "Zyie",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "assetpack": "./bin/run"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
